{
  "name": "react-ts-playground",
  "version": "0.0.0",
  "description": "Playground for using react with typescript",
  "main": "index.js",
  "author": "Christian Linne",
  "license": "MIT",
  "dependencies": {
    "font-awesome": "^4.7.0",
    "lodash-es": "^4.17.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1"
  },
  "devDependencies": {
    "@types/lodash-es": "4.14.4",
    "@types/react": "^15.0.23",
    "@types/react-dom": "^15.5.0",
    "@types/react-hot-loader": "^3.0.1",
    "@types/react-router-dom": "^4.0.4",
    "@types/webpack-env": "^1.13.0",
    "autoprefixer": "^6.7.7",
    "awesome-typescript-loader": "^3.1.3",
    "concurrently": "^3.4.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy": "^1.16.2",
    "inline-chunk-manifest-html-webpack-plugin": "^1.1.1",
    "js-yaml": "^3.8.3",
    "lodash": "^4.17.4",
    "log4js": "^1.1.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.2",
    "normalize.css": "^7.0.0",
    "postcss-flexbugs-fixes": "^2.1.1",
    "postcss-loader": "^1.3.3",
    "react-hot-loader": "3.0.0-beta.6",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "string-replace-loader": "^1.2.0",
    "style-loader": "^0.17.0",
    "tslib": "^1.6.1",
    "typescript": "^2.3.2",
    "url-loader": "^0.5.8",
    "wait-for-change": "^1.1.1",
    "webpack": "^2.4.1",
    "webpack-bundle-analyzer": "^2.4.0",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-closure-compiler": "^2.1.4",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0",
    "yargs": "^7.1.0"
  },
  "scripts": {
    "clear": "rimraf .awcache .tmp .dist src/generated",
    "prestart": "rimraf .tmp && npm run translations",
    "start": "concurrently -r \"npm run translations:watch\" \"npm run dev-server\"",
    "dev-server": "cross-env NODE_ENV=development webpack-dev-server --config webpack/main.config.js --hot",
    "prebuild": "rimraf .dist && npm run translations",
    "build": "cross-env NODE_ENV=production webpack --config webpack/main.config.js",
    "prebuild:cc": "rimraf .dist && npm run translations",
    "build:cc": "npm run build -- --env.useClosureCompiler",
    "prod-server": "npm run build && node example-prod-server/server.js .dist",
    "prod-server:cc": "npm run build:cc && node example-prod-server/server.js .dist",
    "translations": "node bin/compile-translations.js \"src/**/*.i18n.yml\" src/generated/translations.ts",
    "translations:watch": "node bin/compile-translations.js \"src/**/*.i18n.yml\" src/generated/translations.ts -w -n"
  },
  "engines": {
    "node": ">=6.10",
    "npm": ">=3.10",
    "yarn": ">=0.20.0"
  }
}
