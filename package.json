{
  "name": "react-ts-playground",
  "version": "0.0.0",
  "description": "Playground for using react with typescript",
  "author": "Christian Linne",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.1",
    "lodash-es": "^4.17.4",
    "normalize.css": "^7.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-intl": "^2.3.0",
    "react-intl-redux": "^0.6.0",
    "react-loadable": "^4.0.3",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.1",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/jest": "^20.0.2",
    "@types/lodash-es": "4.14.4",
    "@types/react": "^15.0.34",
    "@types/react-dom": "15.5.0",
    "@types/react-hot-loader": "^3.0.3",
    "@types/react-intl": "^2.3.1",
    "@types/react-intl-redux": "^0.1.3",
    "@types/react-loadable": "^4.0.1",
    "@types/react-redux": "^4.4.45",
    "@types/react-router": "^4.0.12",
    "@types/react-router-dom": "^4.0.5",
    "@types/webpack-env": "^1.13.0",
    "app-root-path": "^2.0.1",
    "autoprefixer": "^7.1.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.0.1",
    "cli-highlight": "1.1.4",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.2",
    "dotenv": "4.0.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.4",
    "fs-extra": "3.0.1",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.29.0",
    "http-proxy": "^1.16.2",
    "inline-chunk-manifest-html-webpack-plugin": "^1.1.1",
    "jest": "^20.0.4",
    "js-yaml": "^3.8.4",
    "lodash": "^4.17.4",
    "log4js": "^1.1.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "promise": "^8.0.0",
    "react-dev-utils": "^3.0.2",
    "react-error-overlay": "^1.0.9",
    "react-hot-loader": "3.0.0-beta.6",
    "resolve-url-loader": "^2.1.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "script-ext-html-webpack-plugin": "^1.8.3",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "stylelint": "^7.12.0",
    "stylelint-webpack-plugin": "^0.8.0",
    "sw-precache-webpack-plugin": "^0.11.3",
    "ts-jest": "^20.0.6",
    "ts-loader": "^2.2.2",
    "tslib": "^1.7.1",
    "tslint": "^5.4.3",
    "tslint-react": "^3.0.0",
    "typescript": "^2.4.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-closure-compiler": "^2.1.4",
    "webpack-dev-server": "^2.5.0",
    "webpack-manifest-plugin": "1.1.0",
    "webpack-merge": "^4.1.0",
    "yargs": "^8.0.2"
  },
  "scripts": {
    "prestart": "npm run translations && npm run renderLoadingAnimation",
    "start": "node scripts/start.js",
    "prebuild": "rimraf build && npm run translations && npm run renderLoadingAnimation",
    "build": "node scripts/build.js",
    "build:cc": "npm run build -- --use-closure-compiler",
    "serve": "node scripts/serve.js",
    "test:ci": "node scripts/test.js --env=jsdom --no-cache --verbose --coverage",
    "test": "node scripts/test.js --env=jsdom --verbose",
    "renderLoadingAnimation": "node scripts/util/renderLoading.js",
    "-----need to translate these scripts!-----": "echo 0",
    "analyze:build": "npm run build -- --env.analyzeBundles",
    "clear": "rimraf .awcache .tmp .dist src/generated",
    "build:dev": "npm run prestart && cross-env NODE_ENV=development webpack --config webpack/main.config.js",
    "translations": "node scripts/translations/compile-translations.js \"src/**/*.i18n.yml\" src/generated/translations.ts"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      ".d.ts"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.s?css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^(?!.*\\.(css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "mapCoverage": true
  },
  "engines": {
    "node": ">=6.10",
    "npm": ">=3.10",
    "yarn": ">=0.20.0"
  }
}
