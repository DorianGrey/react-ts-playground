  },
  "dependencies": {
    "lodash-es": "^4.17.11",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-intl": "^2.7.2",
    "react-intl-redux": "^2.0.2",
    "react-loadable": "^5.5.0",
    "react-md": "1.8.0",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "4.0.1",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.3.3",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "start": "webpack-dev-server --config config/webpack/resolvedConfig.dev.js",
    "build": "webpack --config config/webpack/resolvedConfig.prod.js",
    "serve": "node scripts/serve.js",
    "test:ci": "node scripts/test.js --no-cache --coverage",
    "test": "node scripts/test.js",
    "inspect": "node scripts/inspect.js",
    "format": "prettier --write \"{src,scripts,config}/**/*.{js,ts,tsx}\"",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix -t codeFrame && stylelint \"src/**/*.scss\" --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test:ci"
    }
  },
  "lint-staged": {
    "{src,scripts,config}/**/*.{ts,tsx}": [
      "tslint -c tslint.json -p tsconfig.json --fix -t codeFrame",
      "prettier --write",
      "git add"
    ],
    "{src,scripts,config}/**/*.js": [
      "prettier --write",
      "git add"
    ],
    "assets/styles/**/*.scss": [
      "stylelint --fix"
    ]
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">=5.10",
    "yarn": ">=1.0.0"
  },
  "jest-junit": {
    "output": "./test-results/junit/junit.xml"
  }
}
